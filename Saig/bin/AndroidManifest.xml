<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.awp.saig"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="de.saig.activities.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="de.saig.activities.Register"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="de.saig.activities.Overview"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="de.saig.activities.ExternalLogins"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="de.saig.activities.MediaOverview"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="de.saig.activities.AudioRecording"
            android:label="@string/app_name" >
        </activity>
        

    </application>

</manifest>
